<%@page import="limaHeat.bean.Usuario"%>
<%@ page pageEncoding="UTF-8" %>

<%
	Usuario logged = (Usuario) request.getSession().getAttribute("usuario_logged");
	
%>

<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<link href="jq/bootstrap/bootstrap.min.css" rel="stylesheet" type="text/css"/>
<link href="jq/bootstrap/bootstrap-icons.min.css" rel="stylesheet" type="text/css"/>
<link rel="stylesheet" href="css/general.css"/>

<style>
    .row-info {
        background-color: #ce5250;
    }
</style>

<div class="row row-info">
    <div class="col col-md-2">
           <img class="logo" src="data:image/png;base64," alt="Logo">
      
    </div>
    <div class="col col-md-7"></div>
    <div class="col col-md-3">
        <h5 style="margin-top: 15px">¡Hola <%=logged.getApellido1()+" "+logged.getApellido2()+" "+logged.getNombres()%>!</h5>
        <input type="button" value="Cerrar Sesion" id="btnCerrarSesion"/>
    </div>
</div>

<!-- Modal cerrar sesion -->
<div id="modal-cerrar-sesion" class="modal fade" role="dialog">
    <div class="modal-dialog  modal-dialog-centered modal-sm ">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header-color">
                <center style="width: 100%;font-size: 20px;margin-top: 15px">LIMA HEAT</center>
            </div>
            <div class="modal-body">
                <p style="color: black;font-size: 15px; text-align: center">¿Seguro que desea cerrar sesion?</p>
            </div>
            <div class="modal-footer btnLogin">
                <button style="width: 30%;font-size: 15px" type="button" id="btnModalCerrarSesionAceptar" data-dismiss="modal">Aceptar</button>
                <button style="width: 30%;font-size: 15px" type="button" id="btnModalCerrarSesionCancelar" data-dismiss="modal">Cancelar</button>
            </div>
        </div>
    </div>
</div>
<!-- Fin Modal -->

<script src="jq/jquery-3.6.0.min.js"></script>
<script>
    
    $("#btnCerrarSesion").click(function () {

        $("#modal-cerrar-sesion").modal("show");

    });
    
    $("#btnModalCerrarSesionAceptar").click(function () {

        $.ajax({
            url: "limaHeatServlet",
            dataType: "json",
            data:{
                accion:"finalizarHTTPSesion"
            },
            success: function (result) {

                window.location.href = "login.jsp";

            }
        });

    });
    
    $("#btnModalCerrarSesionCancelar").click(function () {

        $("#modal-cerrar-sesion").modal("hide");

    });


</script>